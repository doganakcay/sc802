  object qtumpersonel: TOraQuery
    Session = OraSession1
    SQL.Strings = (
      'select T.SIRA_NO,'
      
        '       translate(T.PERSONEL_ADI || '#39' '#39' || T.SOYADI, '#39#304#214#220#199#286#350#39', '#39'I' +
        'OUCGS'#39') AD,'
      '       SUBSTR(T.TC_KIMLIK_NO, -4) S'#305'fre,'
      '       t.kart_no,T.TC_KIMLIK_NO,'
      
        '       case when t.tc_kimlik_no in ('#39'60058070332'#39','#39'31346037276'#39',' +
        #39'21206380710'#39') then 1 else 0 end yetkili'
      '  from per_personel t'
      ' where coalesce(t.isten_ayrildi, '#39'F'#39') <> '#39'T'#39
      '   and t.kart_no is not null'
      ' ORDER BY t.sira_no')
    Left = 480
    Top = 208
  end
  object QPER_KAYIT: TOraQuery
    SQLInsert.Strings = (
      'INSERT INTO DA_KART_PER_KAYIT'
      '  (GIRIS_ID, PER_NO, AD_SOYAD, CIHAZ_IP, KART_NO)'
      'VALUES'
      '  (:GIRIS_ID, :PER_NO, :AD_SOYAD, :CIHAZ_IP, :KART_NO)')
    SQLDelete.Strings = (
      'DELETE FROM DA_KART_PER_KAYIT'
      'WHERE'
      '  GIRIS_ID = :Old_GIRIS_ID')
    SQLUpdate.Strings = (
      'UPDATE DA_KART_PER_KAYIT'
      'SET'
      
        '  GIRIS_ID = :GIRIS_ID, PER_NO = :PER_NO, AD_SOYAD = :AD_SOYAD, ' +
        'CIHAZ_IP = :CIHAZ_IP, KART_NO = :KART_NO'
      'WHERE'
      '  GIRIS_ID = :Old_GIRIS_ID')
    SQLLock.Strings = (
      
        'SELECT GIRIS_ID, PER_NO, AD_SOYAD, CIHAZ_IP, KART_NO FROM DA_KAR' +
        'T_PER_KAYIT'
      'WHERE'
      '  GIRIS_ID = :Old_GIRIS_ID'
      'FOR UPDATE NOWAIT')
    SQLRefresh.Strings = (
      
        'SELECT GIRIS_ID, PER_NO, AD_SOYAD, CIHAZ_IP, KART_NO FROM DA_KAR' +
        'T_PER_KAYIT'
      'WHERE'
      '  GIRIS_ID = :GIRIS_ID')
    SQLRecCount.Strings = (
      'SELECT Count(*) FROM ('
      'SELECT * FROM DA_KART_PER_KAYIT'
      ''
      ')')
    Session = OraSession1
    SQL.Strings = (
      'select * from DA_KART_PER_KAYIT')
