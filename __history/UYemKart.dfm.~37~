  object QOGUN: TOraQuery
    Session = OraSession1
    SQL.Strings = (
      'SELECT *'
      '  FROM DA_YMK_OGUN T'
      
        ' WHERE :SAAT BETWEEN TO_NUMBER(REPLACE(T.BASLAMA_SAAT, '#39':'#39', '#39#39'))' +
        ' AND'
      '       TO_NUMBER(REPLACE(T.BITIS_SAAT, '#39':'#39', '#39#39'))')
    Left = 528
    Top = 72
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'SAAT'
        Value = nil
      end>
