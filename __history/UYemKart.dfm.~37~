  object qkayit: TOraQuery
    Session = OraSession1
    SQL.Strings = (
      ''
      'select distinct p.kart_no,'
      '                p.tc_kimlik_no,'
      '                p.personel_adi || '#39' '#39' || p.soyadi ad_soyad,'
      
        '                s.saat_aralik,to_number(to_char(sysdate, '#39'hh24MI' +
        #39')),'
      '                case'
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'K'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'K'#39') '
      '                    then'
      '                   nvl(s.sabah, 0) '
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'O'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'O'#39') '
      '                    then'
      '                   nvl(s.ogle, 0)'
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'A'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'A'#39') '
      '                    then'
      '                   nvl(s.aksam, 0)'
      '                end yemek,'
      '                '
      '                case'
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'K'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'K'#39') '
      '                    then'
      '                   '#39'K'#39
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'O'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'O'#39')'
      '                    then'
      '                   '#39'O'#39
      
        '                  when to_number(to_char(sysdate, '#39'hh24MI'#39')) bet' +
        'ween '
      
        '                    (select TO_NUMBER(REPLACE(TT.BASLAMA_SAAT,'#39':' +
        #39','#39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'A'#39') '
      '                    and '
      
        '                    (select TO_NUMBER(REPLACE(TT.BITIS_SAAT,'#39':'#39',' +
        #39#39')) from DA_YMK_OGUN TT WHERE TT.SIMGE='#39'A'#39')'
      '                    then'
      '                   '#39'A'#39
      '                end ogun'
      '  from PDK_PERSONEL_VARDIYALARI t,'
      '       pdk_vardiyalar           v,'
      '       nobet_per_saati          s,'
      '       per_personel             p'
      ' where s.vardia_kodu=v.vardiya_kodu'
      '   and v.vardiya_kodu = t.vardiye_kodu'
      '   and t.tc_kimlik_no = p.tc_kimlik_no'
      '   and t.tarih = trunc(sysdate)'
      '   AND NVL(S.SAAT,0)>0'
      '   AND ROWNUM<2'
      ' and p.kart_no=:kart_id'
      '')
    Left = 520
    Top = 136
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'kart_id'
        Value = nil
      end>
  end
